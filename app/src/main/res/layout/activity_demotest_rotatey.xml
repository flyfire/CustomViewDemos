<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:id="@+id/left_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.15"/>

    <android.support.constraint.Guideline
        android:id="@+id/right_guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.85"/>

    <TextView
        android:id="@+id/tv1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatey 45"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>

    <TextView
        android:id="@+id/tv2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv1"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatey 90"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>

    <TextView
        android:id="@+id/tv3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@id/tv2"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatey 135"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv4"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@id/tv3"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatey 180"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv5"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv4"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatey 270"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv6"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv5"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatex 45"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv7"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv6"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatex 90"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv8"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv7"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatex 135"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv9"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv8"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatex 180"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>
    <TextView
        android:id="@+id/tv10"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@+id/left_guideline"
        app:layout_constraintRight_toLeftOf="@+id/right_guide"
        app:layout_constraintTop_toBottomOf="@+id/tv9"
        android:layout_marginTop="30dp"
        android:textSize="12dp"
        android:textColor="@color/c_FD6090"
        android:text="rotatex 270"
        android:gravity="center"
        android:background="@color/c_76D5FF"/>



</android.support.constraint.ConstraintLayout>